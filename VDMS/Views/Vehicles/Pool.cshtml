@model IEnumerable<VDMS.ViewModel.VehicleViewModel>

@{
    ViewBag.Title = "Pool";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Manufacture.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Owner.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EngineNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChassiNo)
        </th>        
        <th>
            @Html.DisplayNameFor(model => model.SeatingCapacity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CylinderCapacity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ManufacturedYear)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Manufacture.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Owner.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EngineNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChassiNo)
        </td>       
        <td>
            @Html.DisplayFor(modelItem => item.SeatingCapacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CylinderCapacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ManufacturedYear)
        </td>
        <td>
            @Html.ActionLink("Assing", "Create", "ReceiverRecord")
        </td>
    </tr>
}

</table>
